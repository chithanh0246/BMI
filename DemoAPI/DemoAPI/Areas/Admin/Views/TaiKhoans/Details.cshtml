@model Lib.Entities.TaiKhoan

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="content-wrapper">
    <!-- Main content -->
    <section class="content my-5">

        <!-- Default box -->
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col-md-6">
                        <h4>Admin Detail</h4>
                    </div>
                    <div class="col-md-6 text-right">
                        @Html.ActionLink("Edit", "Edit", new { id = Model.IdTK }) |
                        @Html.ActionLink("Back to List", "Index")
                    </div>
                </div>
            </div>
            <div class="card-body px-5">
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Email)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Email)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Pass)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Pass)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.HoTen)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.HoTen)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.IdLoai)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.IdLoai)
                    </dd>

                </dl>

                <!-- /.card-body -->
            </div>
            <!-- /.card -->

    </section>
    <!-- /.content -->
</div>

